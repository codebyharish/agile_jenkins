pipeline {
    agent any

    stages {

        stage('Checkout') {
            steps {
                echo 'Code checked from Git repository'
            }
        }

        stage('Build') {
            steps {
                echo 'Building project from Jenkinsfile'
            }
        }

        stage('Test') {
            steps {
                echo 'Testing project'
            }
        }
    }
}
